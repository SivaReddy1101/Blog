<%- include('partials/header'); -%>
<h1>Compose</h1>
<form action="/compose" method="post">
  <div class="form-group">
    <label for="inputData">Title</label><br />
    <input type="text" name="titleContent" class="form-control" /><br />
  </div>
  <div class="form-group">
    <label for="postContent">Post</label><br />
    <textarea name="postContent" id="" cols="150" rows="10" class="form-control"></textarea><br />
  </div>
  <button type="submit" name="button" class="btn btn-primary">Publish</button>
</form>
